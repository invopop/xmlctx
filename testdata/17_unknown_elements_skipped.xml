<?xml version="1.0" encoding="UTF-8"?>
<!-- Test file demonstrating that unknown elements are skipped (continue case) -->
<user xmlns="http://example.com/schema/user"
      xmlns:ns1="http://example.com/schema/profile"
      xmlns:ns2="http://example.com/schema/address"
      xmlns:extra="http://example.com/unknown"
      id="user-123" version="1.0">
  <!-- Unknown element before name -->
  <unknown-element>This should be skipped</unknown-element>

  <name>John Doe</name>

  <!-- Unknown element with attributes -->
  <extra-data type="internal" priority="high">Should be ignored</extra-data>

  <email>john.doe@example.com</email>

  <!-- Unknown namespaced element -->
  <extra:metadata>
    <extra:field>value</extra:field>
  </extra:metadata>

  <ns1:profile visibility="public" verified="true">
    <!-- Unknown element inside profile -->
    <ns1:deprecated-field>Old field</ns1:deprecated-field>

    <ns1:bio>Software engineer and open source enthusiast</ns1:bio>

    <!-- Another unknown element -->
    <ns1:internal-rating>5</ns1:internal-rating>

    <ns1:avatar-url>https://example.com/avatars/john.jpg</ns1:avatar-url>

    <ns1:tag>developer</ns1:tag>
    <ns1:tag>golang</ns1:tag>
    <ns1:tag>xml</ns1:tag>
  </ns1:profile>

  <ns1:settings>
    <ns1:theme>dark</ns1:theme>
    <ns1:language>en-US</ns1:language>

    <!-- Complex unknown element with nested structure -->
    <ns1:advanced-config>
      <ns1:level1>
        <ns1:level2>
          <ns1:level3>Deep nesting</ns1:level3>
        </ns1:level2>
      </ns1:level1>
    </ns1:advanced-config>

    <ns1:notification enabled="true">
      <ns1:email>true</ns1:email>
      <ns1:push>false</ns1:push>
      <ns1:frequency>daily</ns1:frequency>
    </ns1:notification>
  </ns1:settings>

  <!-- Unknown element from different namespace -->
  <extra:tracking-info>
    <extra:source>web</extra:source>
    <extra:session-id>abc123</extra:session-id>
  </extra:tracking-info>

  <ns2:address type="home" primary="true">
    <ns2:street>123 Main Street</ns2:street>

    <!-- Unknown element in address -->
    <ns2:geocode>
      <ns2:lat>37.7749</ns2:lat>
      <ns2:lng>-122.4194</ns2:lng>
    </ns2:geocode>

    <ns2:city>San Francisco</ns2:city>
    <ns2:state>CA</ns2:state>
    <ns2:zip-code>94102</ns2:zip-code>
    <ns2:country>USA</ns2:country>
  </ns2:address>

  <ns2:metadata>
    <ns2:created-at>2024-01-15T10:30:00Z</ns2:created-at>
    <ns2:updated-at>2024-01-19T14:20:00Z</ns2:updated-at>
    <ns2:source>web-app</ns2:source>

    <!-- Unknown element with chardata and attributes -->
    <ns2:internal-version build="1234">v2.5.1</ns2:internal-version>

    <ns2:custom-field key="department" type="string">Engineering</ns2:custom-field>
    <ns2:custom-field key="employee-id" type="number">12345</ns2:custom-field>
  </ns2:metadata>

  <!-- Unknown element at the end -->
  <audit-trail>
    <entry timestamp="2024-01-20">Modified</entry>
  </audit-trail>
</user>
